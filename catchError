
pipeline {
agent any
stages {
stage ('build'){
steps {
catchError(buildResult: 'FAILURE', stageResult: 'FAILURE') {
sh 'sleep5 ; echo "this is build stage"'
	}
		}
			}
stage ('test'){
steps{

sh 'sleep 5; echo "this is test stage"'
	}
		}
			
stage ('deploy'){
steps{

sh 'sleep 5; echo "this is deploy stage"'
	
		}
			}
				}

					}
